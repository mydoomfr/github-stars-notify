{
  "id": null,
  "title": "GitHub Stars Notify Dashboard",
  "tags": ["github", "stars", "monitoring"],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Total Stars per Repository",
      "type": "stat",
      "targets": [
        {
          "expr": "github_stars_total",
          "legendFormat": "{{owner}}/{{repo}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "pluginVersion": "8.0.0",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
