    {
      "id": 6,
      "title": "GitHub API Rate Limit Usage",
      "type": "timeseries",
      "targets": [
        {
          "expr": "github_api_rate_limit_limit{resource=\"core\"} - github_api_rate_limit_remaining{resource=\"core\"}",
          "legendFormat": "Used",
          "refId": "A"
        },
        {
          "expr": "github_api_rate_limit_limit{resource=\"core\"}",
          "legendFormat": "Limit (core)",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 1,
            "fillOpacity": 0,
            "gradientMode": "none",
            "spanNulls": true,
            "insertNulls": false,
            "showPoints": "auto",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 45
              },
              {
                "color": "orange", 
                "value": 55
              },
              {
                "color": "red",
                "value": 4500
              }
            ]
          },
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Limit (core)"
            },
            "properties": [
              {
                "id": "custom.drawStyle",
                "value": "line"
              },
              {
                "id": "custom.lineStyle",
                "value": {
                  "dash": [5, 5],
                  "fill": "dash"
                }
              },
              {
                "id": "custom.lineWidth",
                "value": 2
              },
              {
                "id": "custom.spanNulls",
                "value": true
              },
              {
                "id": "custom.insertNulls",
                "value": false
              },
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName", 
              "options": "Used"
            },
            "properties": [
              {
                "id": "custom.fillOpacity",
                "value": 20
              },
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "blue"
                }
              }
            ]
          }
        ]
      },
